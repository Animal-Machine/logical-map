  "homepage": ".",
  "engines": {
    "node": "17.9.0"
  },
  "dependencies": {
    "json-server": "^0.17.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
